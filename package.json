{
  "name": "code-fly",
  "version": "1.0.0",
  "description": "文档链接 [剑指 Offer 题解](https://github.com/CyC2018/CS-Notes/blob/master/notes/%E5%89%91%E6%8C%87%20Offer%20%E9%A2%98%E8%A7%A3%20-%20%E7%9B%AE%E5%BD%95.md)",
  "main": "03.js",
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-angular": "^12.1.1",
    "@umijs/fabric": "^2.5.7",
    "@umijs/yorkie": "^2.0.5",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.24.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^3.0.9",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "2.2.1",
    "standard-version": "^9.2.0"
  },
  "scripts": {
    "test": "jest --coverage",
    "commit": "git-cz",
    "release": "standard-version",
    "lint-staged": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkvern/code-fly.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tkvern/code-fly/issues"
  },
  "homepage": "https://github.com/tkvern/code-fly#readme",
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts}": "eslint --ext --fix",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
